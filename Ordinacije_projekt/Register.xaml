<Window x:Class="Ordinacije_projekt.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ordinacije_projekt"
        mc:Ignorable="d"
        Title="Register" Height="323" Width="352">
    <Grid>
        <TextBox x:Name="username_box" HorizontalAlignment="Left" Margin="55,59,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Content="Registreraj" HorizontalAlignment="Left" Margin="55,197,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <PasswordBox x:Name="password1_box" HorizontalAlignment="Left" Margin="55,99,0,0" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="password2_box" HorizontalAlignment="Left" Margin="55,134,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Nazaj" HorizontalAlignment="Left" Margin="176,192,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
    </Grid>
</Window>
